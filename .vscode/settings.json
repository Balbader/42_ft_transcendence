{
	"prettier.useTabs": true,
	"editor.guides.highlightActiveIndentation": "always",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"notebook.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"vim.enableNeovim": true,
	"editor.tabSize": 4,
	"editor.fontFamily": "'FiraCode Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
	"editor.renderLineHighlight": "none",
	"editor.renderLineHighlightOnlyWhenFocus": false,
	"explorer.fileNesting.enabled": true,
	"gitlens.views.fileHistory.avatars": false,
	"markdown.editor.pasteUrlAsFormattedLink.enabled": "always",
	"files.autoSave": "onFocusChange",
	"editor.fontLigatures": true,
	"eslint.format.enable": true,
	"eslint.probe": [
		"astro",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"mdx",
		"vue",
		"markdown",
		"json",
		"jsonc",
		"jsx"
	],
	"eslint.run": "onSave",
	"emmet.showSuggestionsAsSnippets": true,
	"emmet.triggerExpansionOnTab": true,
	"emmet.useInlineCompletions": true,
	"eslint.lintTask.enable": true,
	"eslint.useESLintClass": true,
	"notebook.formatOnSave.enabled": true,
	"color-highlight.markerType": "background",
	"highlightLine.borderStyle": "solid",
	"highlightLine.borderColor": "#535353",
	"highlightLine.borderWidth": ".1px",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.configPath": "",
	"workbench.editor.languageDetection": false,
	"workbench.editor.historyBasedLanguageDetection": false,
	"workbench.commandPalette.experimental.enableNaturalLanguageSearch": false,
	"editor.defaultFoldingRangeProvider": "esbenp.prettier-vscode",
	"eslint.codeActionsOnSave.rules": null
}
